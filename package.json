{
  "private": true,
  "name": "pcc-data",
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider NODE_ENV=production preact build && cp _redirects build/_redirects",
    "serve": "sirv build --cors --single",
    "dev": "cross-env NODE_OPTIONS=--openssl-legacy-provider preact watch --refresh",
    "lint": "eslint src",
    "test": "jest"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "preact",
      "plugin:@typescript-eslint/recommended"
    ],
    "ignorePatterns": [
      "build/"
    ]
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "papaparse": "^5.4.1",
    "preact": "^10.17.1",
    "preact-router": "^4.1.2"
  },
  "devDependencies": {
    "@types/papaparse": "^5.3.8",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "autoprefixer": "^10.4.15",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.1",
    "eslint": "^8.48.0",
    "eslint-config-preact": "^1.3.0",
    "jest": "^29.6.4",
    "postcss": "^8.4.29",
    "postcss-load-config": "^4.0.1",
    "preact-cli": "^3.5.0",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "sirv-cli": "^2.0.2",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.ts",
      "<rootDir>/tests/__mocks__/setupTests.ts"
    ]
  }
}
